{
  "name": "react-pwa",
  "version": "1.0.0",
  "description": "React progressive web application",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:vendor && npm run build:bundle && npm run precache",
    "build:bundle": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:vendor": "NODE_ENV=production webpack --config webpack/webpack.dll.js",
    "clean": "rimraf public/*",
    "precache": "babel-node scripts/precache.js",
    "start": "BABEL_ENV=node NODE_ENV=production babel-node server/server.js",
    "start:dev": "nodemon --exec 'babel-node server/server.js'",
    "test": "jest"
  },
